#include <stdio.h>
#include <stdlib.h>
int main()
{
  long long int n;
  long long int j;
  long long int i;
  long long int s = 0;
  long long int p = 0;
  scanf("%I64d", &n);
  long long int arr[n + 1];
  for (i = 0; i < n; i++)
    scanf("%I64d", &arr[i]);

  for (i = 0; i < n; i++)
  {
    s = s + abs(arr[i]);
    p = p - arr[i];
    arr[i + 1] = arr[i + 1] + p;
  }

  printf("%I64d", s);
  return 0;
}

